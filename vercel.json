{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "agent.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)\\.(css|js|mjs|map|png|jpg|jpeg|gif|svg|ico|webp|json|txt|webmanifest)", "dest": "/$1.$2" },
    { "src": "/sw\\.js", "dest": "/sw.js" },
    { "src": "/manifest\\.json", "dest": "/manifest.json" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}